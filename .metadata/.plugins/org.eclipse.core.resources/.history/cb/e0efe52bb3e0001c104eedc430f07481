package Streams;

import java.util.ArrayList;

public class maxmin {

	public static void main(String[] args) {
		ArrayList<Integer> al=new ArrayList<>();
		al.add(0);
		al.add(10);
		al.add(20);
		al.add(30);
		al.add(20);
		System.out.println(al);
		Int min=al.stream().min((i1,i2)->i1.compareTo(i2)).get();
		

	}

}
